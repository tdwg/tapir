<?xml version="1.0" encoding="UTF-8"?>
<searchTemplate xmlns="http://rs.tdwg.org/tapir/1.0" 
	                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			                xmlns:xs="http://www.w3.org/2001/XMLSchema" 
					                xsi:schemaLocation="http://rs.tdwg.org/tapir/1.0
							                                    http://rs.tdwg.org/tapir/1.0/schema/tapir.xsd
											                                        http://www.w3.org/2001/XMLSchema
																                                    http://www.w3.org/2001/XMLSchema.xsd">
	<label>Dataset title query for ABCD 1.20</label>
	<documentation>
		Query template (ABCD 1.20) to search for records by dataset title.
	</documentation>
	<externalOutputModel location="http://rs.tdwg.org/tapir/cs/abcd/1.20/model/abcd120.xml"/>
		<filter>
			<equals>
				<concept id="http://www.tdwg.org/schemas/abcd/1.2/DataSets/DataSet/OriginalSource/SourceName"/>
				<parameter name="datasetTitle"/>
			</equals>
		</filter>
</searchTemplate>
